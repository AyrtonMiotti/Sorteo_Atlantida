<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CHAT</title>
    <link rel="stylesheet" href="./resources/css/styles.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Jua&display=swap" rel="stylesheet">
</head>
<body>
    <div class="contenedorPrincipal">
        <div class="nabvar">
            <h3>Chat</h3>
        </div>
        <div class="second-container">
            <div class="left-container">
                <div class="leftUp-container">
                    <!-- Message Container-->
                    <div class="full-chat-block">
                        <div class="outer-container">
                            <div class="chat-container">
                                <!-- Messages -->
                                <div id="chatbox">
                                    <% if(time){ %>
                                        <h5 id="chat-time"> <%= time %></h5>
                                    <% } %>

                                    <p id="botStarterMessage" class="botText"><span>Hola sho soy el bot...</span></p>
                                    <p id="botStarterMessage" class="userText"><span>Hola sho soy el user...</span></p>

                                    <% if(data){ %>
                                        <p id="botStarterMessage" class="botText"><span> <%= %></span></p>
                                    <% } %>
                                    
                                    <% if(data){ %>
                                        <p id="botStarterMessage" class="userText"><span><%= %></span></p>
                                    <% } %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div> <!--Cont izq -->
                <div class="leftBottom-container">
                    <% if(data){ %>
                        <% data.forEach((botones=> { %>
                            <a href=""><button class="txtBtn2" > <%= botones[0] %></button></a>
                            <a href=""><button class="txtBtn2" > <%= botones[1] %></button></a>
                            <a href=""><button class="txtBtn2" > <%= botones[2] %></button></a>
                            <a href=""><button class="txtBtn2" > <%= botones[3] %></button></a>
                            <a href=""><button class="txtBtn2" > <%= botones[4] %></button></a>
                            <a href=""><button class="txtBtn2" > <%= botones[5] %></button></a>
                            <a href=""><button class="txtBtn2" > <%= botones[6] %></button></a>
                        <% })) %>
                    <% } %>
                </div>
            </div>
            <div class="right-container">
                <div class="containerImage">
                    <img src="./resources/assets/Group 7.png" class="image">
                </div>
            </div>
        </div>
    </div>
</body>
</html>

